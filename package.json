{
  "name": "budget-tracker",
  "version": "1.0.0",
  "description": "A comprehensive personal finance management application",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:demo": "cross-env USE_DEMO_MODE=true concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "lint": "eslint .",
    "init-db": "node server/init-db.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/BudgetTracker.git"
  },
  "keywords": [
    "budget",
    "finance",
    "expense-tracker",
    "mern"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "react-beautiful-dnd": "^13.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.12.0",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^2.0.22",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "^2.0.0",
    "supertest": "^6.3.3",
    "typescript": "^5.0.4"
  }
}
